{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "astro_generator",
    "astro_token",
    "autostake_lp_tokens",
    "bond_cost_in_utility_tokens",
    "community_pool",
    "governance",
    "pairs",
    "psi_token",
    "vesting",
    "vesting_period"
  ],
  "properties": {
    "astro_generator": {
      "type": "string"
    },
    "astro_token": {
      "type": "string"
    },
    "autostake_lp_tokens": {
      "type": "boolean"
    },
    "bond_cost_in_utility_tokens": {
      "$ref": "#/definitions/Decimal"
    },
    "community_pool": {
      "type": "string"
    },
    "governance": {
      "type": "string"
    },
    "pairs": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "psi_token": {
      "type": "string"
    },
    "utility_token": {
      "type": [
        "string",
        "null"
      ]
    },
    "vesting": {
      "type": "string"
    },
    "vesting_period": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    }
  }
}
